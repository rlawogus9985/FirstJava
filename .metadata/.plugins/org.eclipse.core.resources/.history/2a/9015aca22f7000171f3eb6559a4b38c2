package com.bit.hello;

public class Value03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int intNum1 = 0;
		
		intNum1 = intNum1 + 1;//전통적인 프로그램
		intNum1 += 1; // C,Java 3세대 이상 프로그램언어
		intNum1 += 10;
		
		intNum1 ++; 
		++intNum1 ;
		System.out.println(intNum1);
		
	}

}
